{
    "name": "6ithu/apollo-fork",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Author",
            "name": "6IT",
            "email": "info@6it.hu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metapp\\Apollo\\": "src/"
        }
    },
    "require": {
        "php": ">=8",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-gd": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "doctrine/doctrine-module": "5.*",
        "league/container": "4.2.0",
        "monolog/monolog": "3.2.0",
        "guzzlehttp/psr7": "2.4.1",
        "league/route": "5.1.2",
        "twig/twig": "3.4.3",
        "doctrine/orm": "2.12.2",
        "beberlei/doctrineextensions": "1.3.0",
        "firebase/php-jwt": "6.3.2",
        "phpmailer/phpmailer": "^6.6",
        "php-curl-class/php-curl-class": "9.8.0",
        "laminas/laminas-servicemanager": "3.*",
        "laminas/laminas-i18n": "2.19.0",
        "laminas/laminas-mvc-i18n": "1.*",
        "guzzlehttp/guzzle": "7.5.0",
        "gedmo/doctrine-extensions": "3.9.0",
        "laminas/laminas-diactoros": "^2.19",
        "laminas/laminas-httphandlerrunner": "^2.3",
        "cherif/inertia-psr15": "^0.1.0",
        "fullpipe/twig-webpack-extension": "^4.0"
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true
    }
}
